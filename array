package main 

  

import "fmt"

  
func main() { 

    var myarr [2]string 
 

    myarr[0] = "Om"

    myarr[1] = "Omkar"

    fmt.Println("Elements of Array:") 

    fmt.Println("Element 1: ", myarr[0]) 

    fmt.Println("Element 2: ", myarr[1]) 

    arr := [4]string{"Chun", "Chu", "Chun2120", "Chandigarh"}
  
    fmt.Println("\nElements of the array:") 

  

    for i := 0; i < 3; i++ { 

        fmt.Println(arr[i]) 

  

    } 

  
}
